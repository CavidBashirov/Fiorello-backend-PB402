@model SliderCreateVM
<div class="container mt-5">
    <form class="forms-sample" method="post" asp-action="Create" enctype="multipart/form-data">
        <div class="form-group">
            <label for="name">Slider</label>
            <input class="form-control" id="name" asp-for="UploadImages">
            <span asp-validation-for="UploadImages" class="text-danger"></span>
        </div>
        <button type="submit" class="btn btn-primary me-2 text-white">Submit</button>
        <a asp-action="Index" class="btn btn-light">Cancel</a>
    </form>
</div>